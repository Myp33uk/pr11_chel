<Window x:Class="pr11_chel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pr11_chel"
        mc:Ignorable="d"
        Title="Практическая работа №11" Height="237" Width="343">
    <Grid>
        <Menu VerticalAlignment="Top">
            <MenuItem Header="О программе" Click="Info"></MenuItem>
            <MenuItem Header="Выход" Click="Exit"></MenuItem>
        </Menu>
        <GroupBox Header="Регулярные выражения" HorizontalAlignment="Left" Height="170" Margin="10,23,0,0" VerticalAlignment="Top" Width="313">
            <Grid HorizontalAlignment="Left" Height="148" VerticalAlignment="Center" Width="303">
                <ListBox x:Name="resultList" HorizontalAlignment="Center" Height="107" VerticalAlignment="Top" Width="283" Margin="0,10,0,0"/>
                <Button x:Name="button" Content="Найти совпадения" HorizontalAlignment="Center" Margin="0,122,0,0" VerticalAlignment="Top" Click="Button_Click"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
